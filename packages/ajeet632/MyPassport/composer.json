{
    "name": "ajeet632/MyPassport",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "ajeet632",
        "MyPassport"
    ],
    "homepage": "https://github.com/ajeet632/MyPassport",
    "license": "MIT",
    "authors": [
        {
            "name": "Ajeet Kumar",
            "email": "ajeet632",
            "homepage": "https://github.com/ajeet476",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "5.3.*",
        "php": ">=5.6.4",
        "laravel/passport": "^1.0",
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "ajeet632\\MyPassport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ajeet632\\MyPassport\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
